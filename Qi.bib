	Journal = {\mnras},
	Keywords = {celestial mechanics, Kuiper Belt, planets and satellites: formation, planets and satellites: individual: Neptune, Solar system formation},
	Month = dec,
	Pages = {935-940},
	Title = {{Origin and orbital distribution of the trans-Neptunian scattered disc}},
