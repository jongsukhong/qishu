	Eprint = {1602.06573},
	Journal = {\mnras},
	Keywords = {planets and satellites: formation, planets and satellites: gaseous planets, planets and satellites: terrestrial planets, planet-disc interactions, protoplanetary discs, planetary systems},
	Month = may,
	Pages = {2962-2972},
	Primaryclass = {astro-ph.EP},
	Title = {{Did Jupiter's core form in the innermost parts of the Sun's protoplanetary disc?}},
	Volume = 458,
