	Keywords = {binaries: general, planetary systems, planets and satellites: detection, planets and satellites: dynamical evolution and stability, planets and satellites: formation, stars: kinematics and dynamics},
	Month = may,
	Pages = {83},
	Primaryclass = {astro-ph.EP},
	Title = {{On the Origin of Planets at Very Wide Orbits from the Recapture of Free Floating Planets}},
	Volume = 750,
	Year = 2012,
	Bdsk-File-1 = {},
