	Doi = {10.1086/519918},
	Eprint = {0705.2164},
	Journal = {apj},
	Keywords = {Celestial Mechanics, Methods: n-Body Simulations, Stars: Planetary Systems, Solar System: Formation},
	Month = sep,
	Pages = {423-435},
	Title = {{Post-oligarchic Evolution of Protoplanetary Embryos and the Stability of Planetary Systems}},
