	Eid = {26},
	Eprint = {1507.00502},
	Journal = {\aj},
	Keywords = {comets: general, Oort Cloud},
	Month = jul,
	Pages = {26},
	Primaryclass = {astro-ph.EP},
	Title = {{Effect of Stellar Encounters on Comet Cloud Formation}},
	Volume = 150,
	Year = 2015,
	Bdsk-File-1 = {},
