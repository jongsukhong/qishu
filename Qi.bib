	Eprint = {astro-ph/0112092},
	Journal = {Journal of Computational Physics},
	Month = mar,
	Pages = {484-511},
	Title = {{Systolic and hyper-systolic algorithms for the gravitational N-body problem, with an application to Brownian motion}},
	Volume = 185,
	Year = 2003,
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://dx.doi.org/10.1016/S0021-9991(02)00067-0},
	Bdsk-Url-2 = {http://arxiv.org/abs/astro-ph/0112092},
