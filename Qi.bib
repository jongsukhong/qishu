	Date-Added = {2016-01-08 13:16:19 +0000},
	Date-Modified = {2016-01-08 13:16:19 +0000},
	Eprint = {1601.01486},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Earth and Planetary Astrophysics},
	Month = jan,
