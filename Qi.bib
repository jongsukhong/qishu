	Journal = {\apj},
	Keywords = {planets and satellites: formation, stars: kinematics and dynamics},
	Month = jul,
	Pages = {56},
	Primaryclass = {astro-ph.SR},
	Title = {{Was the Sun Born in a Massive Cluster?}},
