	Journal = {Nature Geoscience},
	Month = mar,
	Pages = {177-180},
	Title = {{Water contents of Earth-mass planets around M dwarfs}},
	Volume = 8,
	Year = 2015,
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://dx.doi.org/10.1038/ngeo2372},
