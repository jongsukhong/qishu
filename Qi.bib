	Date-Added = {2016-02-28 12:03:43 +0000},
	Date-Modified = {2016-02-28 12:03:43 +0000},
	Eprint = {1602.07843},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Earth and Planetary Astrophysics},
	Month = feb,
	Primaryclass = {astro-ph.EP},
	Title = {{The dearth of short-period Neptunian exoplanets - a desert in the period-mass/radius planes}},
	Year = 2016,
	Bdsk-File-1 = {},
