	Journal = {\aj},
	Keywords = {PLANETS AND SATELLITES: INDIVIDUAL: PLUTO, SOLAR SYSTEM: FORMATION},
	Month = jul,
	Pages = {420},
	Title = {{The Origin of Pluto's Orbit: Implications for the Solar System Beyond Neptune}},
	Volume = 110,
	Year = 1995,
