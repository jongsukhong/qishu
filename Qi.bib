	Journal = {\aj},
	Keywords = {Kuiper Belt: general, planets and satellites: dynamical evolution and stability},
	Month = feb,
	Pages = {22},
	Primaryclass = {astro-ph.EP},
	Title = {{Evidence for a Distant Giant Planet in the Solar System}},
	Volume = 151,
	Year = 2016,
