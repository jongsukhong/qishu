	Date-Modified = {2016-05-24 18:51:34 +0000},
	Journal = {\icarus},
	Month = may,
	Pages = {13-32},
	Title = {{From the Kuiper Belt to Jupiter-Family Comets: The Spatial Distribution of Ecliptic Comets}},
	Volume = 127,
	Year = 1997,
	Bdsk-Url-1 = {http://dx.doi.org/10.1006/icar.1996.5637},
	Bdsk-Url-2 = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=1997Icar..127...13L&link_type=EJOURNAL&db_key=AST&high=},
