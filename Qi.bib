	Journal = {Celestial Mechanics and Dynamical Astronomy},
	Keywords = {Galactic tide perturbations, Monte Carlo simulation, Long period comets, Oort Cloud},
	Month = sep,
	Pages = {111-132},
	Title = {{Injection of Oort Cloud comets: the fundamental role of stellar perturbations}},
