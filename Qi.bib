	Eid = {20},
	Eprint = {0912.1611},
	Journal = {\apjs},
	Keywords = {Galaxy: general, gravitational lensing: micro, planets and satellites: detection, surveys},
	Month = aug,
	Pages = {20},
	Primaryclass = {astro-ph.EP},
	Title = {{Short-duration Lensing Events. I. Wide-orbit Planets? Free-floating Low-mass Objects? Or High-velocity Stars?}},
	Volume = 201,
	Year = 2012,
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://dx.doi.org/10.1088/0067-0049/201/2/20},
