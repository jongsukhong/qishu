	Date-Modified = {2017-05-04 00:04:12 +0000},
	Doi = {10.1088/1538-3873/128/964/064201},
	Journal = {\pasp},
	Month = jun,
	Number = 6,
	Pages = {064201},
	Title = {{Exocomet Circumstellar Fe I Absorption in the Beta Pictoris Gas Disk}},
	Volume = 128,
	Year = 2016,
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://dx.doi.org/10.1088/1538-3873/128/964/064201},
